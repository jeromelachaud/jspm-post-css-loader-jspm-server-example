{
  "name": "microScafold",
  "version": "1.0.0",
  "description": "ultra minimal scafolding module",
  "main": "index.js",
  "scripts": {
    "start": "concurrent 'npm run serve' 'npm run build_js'",
    "build_js": "watchify ./index.js -output=./bundle.js --verbose",
    "server": "jspm-server"
  },
  "author": "Jerome Lachaud <jerome.lachaud@gmail.com> (http://jeromelachaud.com)",
  "license": "ISC",
  "dependencies": {
    "normalize.css": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^0.1.1",
    "jspm": "^0.16.24",
    "jspm-server": "^1.0.1",
    "postcss-simple-vars": "^1.2.0"
  },
  "jspm": {
    "dependencies": {
      "css": "npm:jspm-loader-css@^1.0.1-beta1",
      "lodash-node": "npm:lodash-node@^3.10.1",
      "postcss-simple-vars": "npm:postcss-simple-vars@^1.2.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
