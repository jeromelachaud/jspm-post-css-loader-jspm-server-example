  "dependencies": {
    "normalize.css": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^0.1.1",
    "jspm": "^0.16.24",
    "live-server": "^0.9.0"
  },
  "jspm": {
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.20",
      "lodash-node": "npm:lodash-node@^3.10.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
